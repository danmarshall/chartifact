{
  "name": "@microsoft/interactive-document-host",
  "version": "1.0.0",
  "description": "ost page to render interactive documents",
  "main": "dist/esnext/index.js",
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/esnext/index.js"
    },
    "./types": {
      "types": "./dist/esnext/types.d.ts",
      "import": "./dist/esnext/types.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "dev": "vite",
    "build-typescript": "tsc -p .",
    "bundle": "vite build --config vite.bundle.config.js",
    "build": "npm run build-typescript && npm run bundle",
    "build:03": "npm run build",
    "preview": "vite preview",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/microsoft/interactive-documents.git"
  },
  "author": "Dan Marshall",
