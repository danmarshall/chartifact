{
  "name": "@microsoft/interactive-document-renderer",
  "type": "module",
  "private": true,
  "version": "1.0.0",
  "description": "Low-code, low-friction interactive documents made with markdown",
  "main": "dist/es2022/index.js",
  "types": "dist/es2022/index.d.ts",
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "deploy": "node ./scripts/deploy.js",
    "watch-typescript": "tsc -p . -w",
    "build-typescript": "tsc -p .",
    "bundle": "vite build --config vite.bundle.config.js",
    "build": "npm run build-typescript && npm run bundle"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/microsoft/interactive-documents.git"
  },
  "keywords": [
    "markdown",
    "vega",
    "vega-lite"
  ],
  "author": "Dan Marshall",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/microsoft/interactive-documents/issues"
  },
  "homepage": "https://github.com/microsoft/interactive-documents#readme"
}
